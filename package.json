{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "dev": "run-p dev:vite dev:serve",
    "dev:vite": "vite",
    "dev:serve": "php artisan serve",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "clean": "rimraf ./node_modules/.vite",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:php": "composer pint",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf public/build bootstrap/ssr",
    "build-only": "vite build && vite build --ssr",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/vue2": "^1.0.6",
    "@logue/vue2-helpers": "2.1.6",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.4",
    "normalize.css": "^8.0.1",
    "tailwindcss": "^3.3.2",
    "vue": "^2.7.14",
    "vue-inertia-composable": "^0.4.3",
    "ziggy-js": "^1.5.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.1",
    "@types/ziggy-js": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/tsconfig": "^0.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "latest",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "laravel-vite-plugin": "^0.7.5",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "typescript": "latest",
    "vite": "^4.3.5",
    "vite-plugin-checker": "^0.6.0",
    "vue-eslint-parser": "^9.2.1",
    "vue-server-renderer": "^2.7.14",
    "vue-tsc": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,jsonc,json5,yaml,yml,toml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*.php": "composer pint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "yaml": "^2.2.2"
  }
}
