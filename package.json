{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "engines": {
    "node": ">=18.12.0",
    "yarn": ">=1.22.10"
  },
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "dev": "run-p dev:vite dev:serve",
    "dev:vite": "vite",
    "dev:serve": "php artisan serve",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "clean": "rimraf ./node_modules/.vite",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:php": "composer pint",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf public/dist",
    "build-only": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/inertia-vue": "^0.8.0",
    "@inertiajs/progress": "^0.2.7",
    "@logue/vue2-helpers": "^2.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.2",
    "normalize.css": "^8.0.1",
    "tailwindcss": "^3.2.4",
    "vue": "^2.7.14",
    "vue-inertia-composable": "^0.3.2",
    "ziggy-js": "^1.5.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@types/ziggy-js": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-vue2": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.2",
    "laravel-vite-plugin": "^0.7.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylelint": "^14.15.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-checker": "^0.5.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,jsonc,json5,yaml,yml,toml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*.php": "composer pint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  }
}
